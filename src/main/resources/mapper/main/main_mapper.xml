<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ustore.employee.dao.GroupManageDao">
	<select id="getEmpInfo" resultType="EmployeeDto">
		SELECT emp_idx, emp_pw 
		FROM employee
		WHERE emp_idx={param1};
	</select>
	<select id="getEmpRoles" resultType="EmpRolesDto">
		SELECT emp_idx, emp_pw 
		FROM employee
		WHERE emp_idx={param1};
	</select>
</mapper>